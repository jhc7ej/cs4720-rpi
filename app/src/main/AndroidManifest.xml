<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marlenakauer.wmbuild1" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.marlenakauer.wmbuild1.Build1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.marlenakauer.wmbuild1.scanPage"
            android:label="@string/title_activity_scan_page" >
        </activity>
        <activity
            android:name="com.example.marlenakauer.wmbuild1.QTscanPage"
            android:label="@string/title_activity_qtscan_page" >
            android:parentActivityName="com.example.marlenakauer.wmbuild1.Build1" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.marlenakauer.wmbuild1.Build1" />
        </activity>
        <activity
            android:name="com.example.marlenakauer.wmbuild1.QRscannerPage"
            android:label="@string/title_activity_qrscanner_page"
            android:parentActivityName="com.example.marlenakauer.wmbuild1.Build1" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.marlenakauer.wmbuild1.Build1" />
        </activity>
    </application>

</manifest>
